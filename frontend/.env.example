# ============================================
# QuickMeet Frontend - Environment Variables
# ============================================
# Instrucciones: Copiar este archivo a .env y ajustar los valores para tu ambiente
# Usar prefijo NG_APP_ para variables que Angular debe leer

# API Configuration
NG_APP_API_URL=http://localhost:5173/api
NG_APP_API_TIMEOUT=30000

# Environment Settings
NG_APP_ENVIRONMENT=development
NG_APP_LOG_LEVEL=debug

# Features
NG_APP_ENABLE_MOCK_API=false
NG_APP_ENABLE_DEBUG_PANEL=true
NG_APP_ENABLE_BETA_FEATURES=true

# Auth
NG_APP_SESSION_TIMEOUT_MINUTES=60

# Analytics (opcional - dejar vac√≠o si no se usa)
NG_APP_GOOGLE_ANALYTICS_ID=
NG_APP_SENTRY_DSN=
